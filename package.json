{
  "name": "gatsby-starter-essentials",
  "version": "0.0.3",
  "description": "A simple Gatsby starter bundled with essential tools",
  "keywords": [
    "gatsby",
    "starter",
    "essentials",
    "mkit"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.{js, ts, jsx, tsx, css}\"",
    "lint": "eslint --fix \"src/**/*.{js, ts, jsx, tsx, css}\"",
    "develop": "gatsby develop --open -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "release": "standard-version"
  },
  "dependencies": {
    "gatsby": "^2.23.0",
    "gatsby-plugin-facebook-pixel": "^1.0.3",
    "gatsby-plugin-google-analytics": "^2.3.3",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-nprogress": "^2.3.3",
    "gatsby-plugin-offline": "^3.2.8",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sitemap": "^2.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.10",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0"
  },
  "author": "MK IT Ltd. <hi@mkit.io>",
  "homepage": "https://github.com/MK-IT/gatsby-starter-essentials",
  "repository": {
    "type": "git",
